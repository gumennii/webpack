{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions"]
        },
        "debug": true
      }
    ],
    "babel-preset-react"
  ],
  "plugins": [
    "syntax-dynamic-import",
    "universal-import",
    "transform-object-rest-spread",
    ["emotion", { "sourceMap": true, "autoLabel": true } ]
  ],
  "env": {
    "development": {
      "plugins": ["react-hot-loader/babel"]
    }
  }
}