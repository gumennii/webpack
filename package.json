    "style-loader": "^0.22.1",
    "uglifyjs-webpack-plugin": "1.3.0",
    "webpack": "^4.16.5",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-express-middleware": "^1.2.1",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-node-externals": "1.7.2"
  }
